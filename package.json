{
    "name": "@jackstenglein/mui-mentions",
    "version": "0.1.11",
    "description": "@mention people in a MUI TextField",
    "main": "./dist/cjs/index.js",
    "module": "./dist/esm/index.js",
    "types": "./dist/esm/index.d.ts",
    "scripts": {
        "start": "parcel examples/src/index.html --dist-dir .parcel-dist",
        "docs": "parcel build ./examples --dist-dir docs --public-url .",
        "build": "npm run build:esm && npm run build:cjs",
        "build:esm": "tsc",
        "build:cjs": "tsc --module commonjs --outDir dist/cjs",
        "lint": "eslint \"{**/*,*}.{js,ts,jsx,tsx}\"",
        "prettier": "prettier --write \"{src,tests,example/src}/**/*.{js,ts,jsx,tsx}\"",
        "test": "jest --config jestconfig.json",
        "prepare": "npm run build",
        "prepublishOnly": "npm test && npm run prettier"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/jackstenglein/mui-mentions.git"
    },
    "keywords": [
        "react",
        "@mention",
        "typescript",
        "MUI",
        "textfield"
    ],
    "author": "Jack Stenglein",
    "license": "MIT",
    "devDependencies": {
        "@emotion/react": "^11.14.0",
        "@emotion/styled": "^11.14.1",
        "@mui/icons-material": "^7.3.2",
        "@mui/material": "^7.3.2",
        "@testing-library/react": "^16.3.0",
        "@types/invariant": "^2.2.37",
        "@types/jest": "^30.0.0",
        "@types/react": "^19.1.13",
        "@typescript-eslint/eslint-plugin": "^8.44.0",
        "@typescript-eslint/parser": "^8.44.0",
        "eslint": "^9.35.0",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-prettier": "^5.5.4",
        "eslint-plugin-react": "^7.37.5",
        "eslint-plugin-react-hooks": "^5.2.0",
        "jest": "^30.1.3",
        "jest-canvas-mock": "^2.5.2",
        "jest-environment-jsdom": "^30.1.2",
        "parcel": "^2.16.0",
        "prettier": "^3.6.2",
        "prettier-plugin-organize-imports": "^4.3.0",
        "process": "^0.11.10",
        "react": "^19.1.1",
        "react-dom": "^19.1.1",
        "ts-jest": "^29.4.3",
        "typescript": "^5.9.2"
    },
    "peerDependencies": {
        "@emotion/react": "^11.11.4",
        "@emotion/styled": "^11.11.0",
        "@mui/material": ">=5.0.0",
        "react": ">=18"
    },
    "files": [
        "dist",
        "LICENSE",
        "README.md"
    ],
    "dependencies": {
        "invariant": "^2.2.4"
    }
}
